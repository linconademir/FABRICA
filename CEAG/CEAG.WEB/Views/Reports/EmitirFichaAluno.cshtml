@model CEAG.WEB.ViewModel.Aluno.AlunoExibicaoViewModel

@{
    ViewBag.Title = "Ficha de Aluno";
    Layout = "~/Views/Shared/_LayoutReport.cshtml";
}

@if (!ViewBag.EmitirContrato)
{
    <br />
    <br />
    <div align="center">
        <a class="btn btn-primary" href="@Url.Action("EmitirFichaAluno","Reports", new { codAlunoMatricula = Model.AlunoMatriculaExibicaoViewModel.CodAlunoMatricula, gerarPDF = true})">
            <b>IMPRIMIR PDF</b>
        </a>
        <a class="btn btn-primary" href="@HttpContext.Current.Request.UrlReferrer.AbsoluteUri">
            <b>VOLTAR</b>
        </a>
        <a class="btn btn-primary" href="@Url.Action("Create","AlunoQuestionarios", new { codAluno = Model.AlunoMatriculaExibicaoViewModel.CodAluno})">
            <b>EDITAR QUESTIONÁRIO</b>
        </a>
    </div>
}
    <div class="containerLopus">    
        <div align="center">
            <table width="850">
                <tr>
                    <td width="28%">
                        <img height="100" src="data:image/png;base64," />
                    </td>
                    <td>
                        <h4><b>CENTRO EDUCACIONAL ANTONIO GALDINO</b></h4>
                    </td>
                </tr>
            </table>
        </div>

        <div align="center"><h4><b>FICHA DO ALUNO</b></h4></div>
        <div align="center"><h4>ANO LETIVO @DateTime.Now.Year</h4></div>
        <div align="center">
            <table border="1" width="850" height="30">
                <tr align="center" class="bg-primary">
                    <td><b>INFORMAÇÕES DO ALUNO</b></td>
                </tr>
            </table>
            <table border="1" width="850" height="40">
                <tr>
                    <td><b>&nbsp;NOME COMPLETO: </b>@Model.Nome.ToUpper()</td>
                </tr>
            </table>
            <table border="1" width="850" height="40">
                <tr>
                    <td><b>&nbsp;DATA DE NASCIMENTO: </b>@Model.NascimentoFormatado</td>
                    <td><b>&nbsp;SÉRIE: </b>@Model.AlunoMatriculaExibicaoViewModel.Turma.Segmento</td>
                    <td><b>&nbsp;TURMA: </b>@Model.AlunoMatriculaExibicaoViewModel.Turma.Periodo</td>
                </tr>
            </table>
            <table border="1" width="850" height="5e0">
                <tr>
                    <td height="20"><b>&nbsp;NOME DA MÃE: </b>@Model.MaeNome.ToUpper()</td>
                    <td><b>&nbsp;TELEFONE: </b>@Model.FoneMae</td>
                </tr>
                <tr>
                    <td height="20"><b>&nbsp;PROFISSÃO DA MÃE: </b>@Model.MaeProfissao.ToUpper()</td>
                    <td><b>&nbsp;TELEFONE: </b>@Model.FoneMaeTrabalho</td>
                </tr>
                <tr>
                    <td height="20"><b>&nbsp;NOME DO PAI: </b>@Model.PaiNome.ToUpper()</td>
                    <td><b>&nbsp;TELEFONE: </b>@Model.FonePai</td>
                </tr>
                <tr>
                    <td height="20"><b>&nbsp;PROFISSÃO DO PAI: </b>@Model.PaiProfissao.ToUpper()</td>
                    <td><b>&nbsp;TELEFONE: </b>@Model.FonePaiTrabalho</td>
                </tr>
            </table>

            <table border="1" width="850" height="30">
                <tr align="center" class="bg-primary">
                    <td width="70%"><b>QUESTIONÁRIO</b></td>
                    <td width="10%"><b>RESPOSTA</b></td>
                    <td width="20%"><b>COMPLEMENTO</b></td>
                </tr>
            </table>
            <table border="1" width="850">
                <tr align="left"></tr>
                @foreach (var itemQuestionario in @Model.AlunoQuestionarioExibicaoViewModels)
                {
                    <tr align="left">
                        <td width="70%" height="20">
                            <b>&nbsp;@itemQuestionario.Ordem.&nbsp;@itemQuestionario.Descricao</b>
                        </td>
                        <td width="10%" align="left">
                            <b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@itemQuestionario.Resposta.ToUpper()</b>
                        </td>
                        <td width="20%" align="left">
                            @if (itemQuestionario.TemComplemento.ToUpper().Equals("SIM") && itemQuestionario.Resposta.ToUpper().Equals("SIM"))
                            {
                                <b>&nbsp;@itemQuestionario.Complemento</b>
                            }
                        </td>
                    </tr>
                }
            </table>
            <table border="1" width="850" height="30">
                <tr align="center" class="bg-primary">
                    <td><b>CONTATOS DE EMERGÊNCIA</b></td>
                </tr>
            </table>
            <table border="1" width="850">
                <tr align="left"></tr>
                @foreach (var itemTelefones in @Model.TelefoneViewModels.Where(p => p.TipoContato.ToUpper().Equals("EMERGÊNCIA")))
                {
                    <tr align="left">
                        <td width="70%" height="30">
                            <b>&nbsp;@itemTelefones.Tipo&nbsp;@itemTelefones.Pessoa&nbsp;@itemTelefones.Local</b>
                        </td>
                        <td width="30%" align="left">
                            <b>&nbsp;(@itemTelefones.Ddd)&nbsp;@itemTelefones.Numero</b>
                        </td>
                    </tr>
                }
            </table>
            <table border="1" width="850" height="30">
                <tr align="center" class="bg-primary">
                    <td><b>COM QUEM VAI PRA CASA</b></td>
                </tr>
            </table>
            <table border="1" width="850">
                <tr align="left"></tr>
                @foreach (var itemTelefones in @Model.TelefoneViewModels.Where(p => p.Pessoa.ToUpper().Equals("TRANSPORTADOR")))
                {
                    <tr align="left">
                        <td width="70%" height="30">
                            <b>&nbsp;@itemTelefones.Tipo&nbsp;@itemTelefones.Local&nbsp;(@itemTelefones.Observacao)</b>
                        </td>
                        <td width="30%" align="left">
                            <b>&nbsp;(@itemTelefones.Ddd)&nbsp;@itemTelefones.Numero</b>
                        </td>
                    </tr>
                }
            </table>
            <table border="1" width="850" height="40">
                <tr align="center" class="bg-primary">
                    <td><b>CONDUÇÃO</b></td>
                </tr>
            </table>
            <table border="1" width="850">
                <tr align="left"></tr>
                @foreach (var itemTelefones in @Model.TelefoneViewModels.Where(p => p.Pessoa.ToUpper().Equals("CONDUÇÃO")))
                {
                    <tr align="left">
                        <td width="70%" height="30">
                            <b>&nbsp;@itemTelefones.Tipo&nbsp;@itemTelefones.Local&nbsp;(@itemTelefones.Observacao)</b>
                        </td>
                        <td width="30%" align="left">
                            <b>&nbsp;(@itemTelefones.Ddd)&nbsp;@itemTelefones.Numero</b>
                        </td>
                    </tr>
                }
            </table>
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />
            &nbsp;
            <table align="center">
                <tr><td align="center"><b>_______________________________________________________</b></td></tr>
                <tr><td align="center"><b>Assinatura do Responsável</b></td></tr>
            </table>
        </div>

    </div>


@section Scripts {

    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/inputmask")
    <script type="text/javascript">
        $(document).ready(function () {
            $("#telefone").inputmask("mask", { "mask": "(99) 9999-9999" });
            $("#cpf").inputmask("mask", { "mask": "999.999.999-99" }, { reverse: true });
            $("#cep").inputmask("mask", { "mask": "99999-999" });
            $("#preco").inputmask("mask", { "mask": "999.999,99" }, { reverse: true });
            $("#valor").inputmask("mask", { "mask": "#.##9,99" }, { reverse: true });
        });
    </script>
}